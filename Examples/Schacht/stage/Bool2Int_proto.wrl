#VRML V2.0 utf8

PROTO Bool2Int [
  exposedField  SFBool    value		FALSE
  eventOut      SFInt32   int		
]
{
  PROTO Data [
    exposedField  SFBool    value		FALSE
  ]
  {
    Group { }
  }

  DEF _bool2Int Script { 
    eventIn   SFBool    set_value
    eventOut  SFInt32   i IS int
    field     SFNode    data DEF Data Data { 
      value IS value
    }
    url "vrmlscript:
function set_value(value, time) { i = value; }
function initialize() { i = data.value; }
"
    directOutput TRUE
  }

  ROUTE Data.value_changed TO _bool2Int.set_value
}

